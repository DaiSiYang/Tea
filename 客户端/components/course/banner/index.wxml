<swiper
    indicator-dots="true"   
    autoplay="true"   
    interval="3000"   
    duration="500"   
    circular="true"  
    indicator-color="#000"  
    indicator-active-color="#fff"
    class="bannerUrl"  
> 
  <swiper-item wx:for="{{bannerList}}" wx:key="{{item.id}}">
    <image src="{{item.url}}" mode="scaleToFill"/>
  </swiper-item>
</swiper>